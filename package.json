{
	"name": "dshima-demo",
	"version": "0.2.4",
	"description": "Agate - Dshima",
	"main": "src/index.js",
	"author": "Agate Gamification",
	"scripts": {
		"start": "node server.js",
		"dev": "webpack server --config webpack/webpack.dev.js --env build=agate-dev",
		"build:stag": "webpack --config webpack/webpack.prod.js --env build=agate-stag",
		"build:dev": "webpack --config webpack/webpack.prod.js --env build=agate-dev",
		"build": "webpack --config webpack/webpack.prod.js --env build=agate-prod",
		"dist": "npx http-server dist/ -p 5500 -e",
		"copy:asset": "npx copyfiles -u 2 'src/assets/**/*.!(templateinfo)' dist/",
		"convert:csvtojson": "npx run-func src/js/module/parsejson/csvtojson.js convert",
		"chrome:cors": "start chrome --disable-web-security --disable-gpu --user-data-dir=temp/chromeTemp",
		"build:run": "npm run build:dev && npm run copy:asset && npm run dist",
		"package-scorm": "node scoPackager.js",
		"build:scorm": "npm run build && npm run package-scorm"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/preset-env": "^7.11.5",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^1.0.1",
		"copy-webpack-plugin": "^6.4.1",
		"css-loader": "^5.2.0",
		"css-minimizer-webpack-plugin": "^3.0.2",
		"file-loader": "^3.0.1",
		"html-webpack-plugin": "^5.3.2",
		"javascript-obfuscator": "^2.19.0",
		"mini-css-extract-plugin": "^0.8.2",
		"optimize-css-assets-webpack-plugin": "^6.0.1",
		"postcss-loader": "^6.1.1",
		"raw-loader": "^1.0.0",
		"simple-scorm-packager": "^0.2.5",
		"style-loader": "^3.3.0",
		"terser-webpack-plugin": "^4.2.3",
		"webpack": "^5.46.0",
		"webpack-cli": "^4.7.2",
		"webpack-dev-server": "^4.3.0",
		"webpack-merge": "^5.2.0",
		"webpack-obfuscator": "^0.18.8"
	},
	"dependencies": {
		"axios": "^0.20.0",
		"convert-csv-to-json": "^1.0.0",
		"crypto-js": "^4.0.0",
		"express": "^4.17.2",
		"func": "^1.3.4",
		"phaser": "^3.55.2",
		"pipwerks-scorm-api-wrapper": "^0.1.2",
		"run": "^1.4.0",
		"run-func": "^1.0.5",
		"tile-extruder": "^2.1.0",
		"uuid": "^8.3.1"
	}
}
